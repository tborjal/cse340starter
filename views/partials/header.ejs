<header id="top-header">
  <span class="siteName">
    <a href="/" title="Return to home page">CSE Motors</a>
  </span>
  <div id="acct-btn">
    <% if(locals.loggedin) { %>
      <a href="/account"><b>Welcome, <%= locals.accountData.account_firstname %></b></a>
      <br>
      <a href="/account/logout">Log Out</a>
      <% } else { %>
        <a title="Click to log in" href="/account/login">My Account</a>
        <% } %>
          <!-- account login path is going to the view -->
  </div>
</header>